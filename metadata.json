{
  "name": "s12v-blackfire",
  "version": "0.1.0",
  "author": "Sergey Novikov",
  "summary": "Module for managing and configuring Blackfire PHP Profiler",
  "license": "Apache-2.0",
  "source": "https://github.com/s12v/puppet-blackfire",
  "project_page": "https://github.com/s12v/puppet-blackfire",
  "issues_url": "https://github.com/s12v/puppet-blackfire/issues",
  "dependencies": [
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppetlabs-inifile",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0"
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem":"RedHat",
      "operatingsystemrelease":[ "5.0", "6.0", "7.0" ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [ "6", "7" ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ "10.04", "12.04", "14.04" ]
    }
  ]
}
